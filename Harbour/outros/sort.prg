clear
alert('')
// array containing non used numbers
aUsedNumbers := array(25)
for nI := 1 to 25
   aUsedNumbers[nI] := nI
next

// array used for the sorting
aArray := {18,14,13,10,6,20,24,15,7,11,17,21,22,1,19,9,23,8,3,25,16,2,12,4,5}
/*
aArray := array(25)
for nI := 1 to 25
   nSelected := int(rand1(1,26)) * (25-nI)
   aArray[nI] := nSelected
   aUsedNumbers[nSelected] := nil
next
*/

nNotSorted := .t.
while nNotSorted
   clear
   nNotSorted := .f.

   for nI := 1 to 25
      if (nI < len(aArray))
         if (aArray[nI] > aArray[nI+1])
            nTemp := aArray[nI]
            aArray[nI]:= aArray[nI+1]
            aArray[nI+1]:= nTemp
            nNotSorted := .t.
         end
      end

      @ nI,00 say replicate('Û', aArray[nI] * 2)
   next
   inkey(0.05)
end

